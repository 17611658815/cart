.isIphoneXBox{
    width: 100%;
    height: 50rpx;
    background: #ffffff;
}
.navtab{
    height: 100rpx;
    background: #ffffff;
    display: flex;
    align-items: center;
    justify-content: space-around;
    z-index: 99999;
    font-size: 22rpx;
    padding-bottom: env(safe-area-inset-bottom);
    
}
.bottomNav{
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 99999
}
.navtab image{
    width: 45rpx;
    height: 45rpx;
}
.dpfC {
    display: flex;
    flex-direction: column;
    align-items: center;
}
.actives{
    color: #000000;
}
.numMsg{
    width: 20rpx;
    height: 20rpx;
    background: #FC252B;
    position: absolute;
    right: -5rpx;
    top: -3rpx;
    border-radius: 50%;
    text-align: center;
    line-height: 20rpx;
    font-size: 15rpx;
    color: #ffffff;
}