
/* 提示一分钟认证 */
.alertText{
    width: 94%;
    padding: 20rpx 3%;
    display: flex;
    align-items: center;
    background: #7DAEFD;
    color: #ffffff;
    font-size: 28rpx;
}
/* 轮播图 */
swiper{
    width: 100%;
    height: 100%;
}
swiper-item {
    width: 100rpx;
    height: 160rpx;
}
.home-swiper {
    width: 100%;
    height: 160rpx;
}

.home-swiper image {
    width: 100%;
    height: 100%;
}
/* 交易数据 */
.tractionData{
    background: #ffffff;
}
.textTitle{
    display: flex;
    width: 60%;
    align-items: center;
    justify-content: center;
    height: 50rpx;
    font-size: 26rpx;
    color: #999999;
    margin-top: 20rpx;
}
.textTitle text{
    white-space: nowrap;
}
.tagBox{
    padding: 0 3%;
    height: 80rpx;
}
.iconBox{
    width: 50rpx;
    height: 50rpx;
    background: #999999;
}
.iconBox image{
    width: 100%;
    height: 100%;
}
.tagBox>view{
    height: 100%;
}