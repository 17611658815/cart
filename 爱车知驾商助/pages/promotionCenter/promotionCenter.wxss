.navList{
    padding: 20rpx 3%  10rpx 3%;
    background: #ffffff;
    border-bottom: 1rpx solid #e2e2e2;
    font-size: 28rpx;
    display: flex;
    justify-content: space-around;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 9999;
}
.navList>view{
    padding-bottom: 10rpx;
}
.navActive{
    border-bottom: 5rpx solid #333333;
}
.icon_3{
    width: 150rpx;
    height: 100rpx;
}
.icon_4{
    width: 30rpx;
    height: 30rpx;
}
.moneySum{
    width: 94%;
    padding: 40rpx 3%;
    display: flex;
    justify-content: space-between;
    background: #ffffff;
}
.moneySum>view{
    flex: 1;
}
.sumBtn{
    display: flex;
    justify-content: flex-end;
}
.sumBtn>view{
    width: 120rpx;
