/* 搜索推广数据 */
.tabListData{
    
}
.dataTab{
    display: flex;
    align-items: center;
    justify-content: center;
}
.dataTab>view{
    border: 1rpx solid #e2e2e2;
    padding: 10rpx 30rpx;
    font-size: 28rpx;
}
.dataTab>view:nth-child(1){
  border-radius: 44rpx 0 0 44rpx;
}
.dataTab>view:nth-child(2){
  border-radius: 0 44rpx 44rpx 0;
}
.acticeClass{
    border: 1rpx solid #81AAC5!important;
    background: #F5F8FF;
    color: #81AAC5;
}
.listBox{
    width: 100%;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    border-bottom: 1rpx solid #e2e2e2;
}
.list_item{
    width: 44%;
    height: 100rpx;
    border: 1rpx solid #e2e2e2;
    border-bottom: none;
    padding: 20rpx;
}
.list_item:nth-child(2n){
    border-left: none;
}


/* 推广筛选 */
.slect_item{
    width: 94%;
    margin-top: 80rpx;
    padding: 20rpx 3%;
    background: #ffffff;
    display: flex;
    align-items: center;
    position: relative;

}
.slect_item>view{
    flex: 1;
    text-align: center;
}
.slect_item image{
    width: 30rpx;
    height: 30rpx;
}
.rotateRight{
  transform: rotate(180deg); 
}
/* 排序下拉 */
.sortList{
    width: 100%;
    display: flex;
    padding: 0 0 20rpx 0;
    flex-direction: column;
    align-items: center;
    background: #ffffff;
    position: absolute;
    top: 75rpx;
    left: 0;
    z-index: 999;
    animation: mysortfirst 0.3s;
}
@keyframes mysortfirst {
    from {
        height: 0rpx;
    }
 
    to {
       height: 290rpx;
    }
}
.sortList_item{
    display: flex;
    color: #999;
    font-size: 28rpx;
    position: relative
}
.sortList_item image{
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    right: 0;
    top: 40%;
}


.typeList_1,.typeList_2,.typeList_3{
    width: 94%;
    padding: 0 3%;
}
/* 冲减卷卡卷*/
.typeList_1 .wrapper {
  display: flex;
  position: relative;
  width: 100%;
  height: 200rpx;
  filter: drop-shadow(4rpx 4rpx 8rpx #F9903F);
  margin-top: 30rpx;
 
}
.typeList_1 .wrapper>view{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  
}
.typeList_2 .wrapper {
  display: flex;
  position: relative;
  width: 100%;
  height: 200rpx;
  filter: drop-shadow(4rpx 4rpx 8rpx #F9903F);
  margin-top: 30rpx;
 
}
.typeList_2 .wrapper>view{
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  
}
.typeList_1 .left>view:nth-child(2){
    margin-top: 50rpx;
}
.typeList_1 .wrapper text{
  display: inline-block;
  position: absolute;
  right: 30%;
  top: 10rpx;
  border: 2rpx solid red;

}
.typeList_1 .right view{
     white-space: nowrap
}
.typeList_1 .ticket_icon{
    width: 100rpx;
    height: 100rpx;
    margin-right: 20rpx;
}
.typeList_1 .left {
     background: #ffffff;
  background: radial-gradient(circle at top right, transparent 10rpx, #F9903F 0) top right, radial-gradient(circle at bottom right, transparent 5px, #F9903F 0) bottom right;
  background-size: 100% 60%;
  background-repeat: no-repeat;
  color: #ffffff;
  width: 70%;
  border-radius: 10rpx 0 0 10rpx;
  position: relative;
  
}

.typeList_1 .right {
    background: #ffffff;
  background: radial-gradient(circle at top left, transparent 10rpx, #F9903F 0) top left,radial-gradient(circle at bottom left, transparent 10rpx, #F9903F 0) bottom left;
  background-size: 100% 60%;
  background-repeat: no-repeat;
  width: 30%;
   color: #ffffff;
  border-radius: 0 10rpx 10rpx 0; 
  display: flex;
  position: relative;
  
}
.typeList_1 .right>view{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%)
}
.typeList_1 .right::after{
  content: "";
  position: absolute;
  top: 10rpx;
  height: 180rpx;
  border-left: 2rpx dashed #e2e2e2;
   background: #F9903F;
}

.typeList_1 .ticket_icon image{
    width: 100%;
    height: 100%;
}
.typeList_1 .tiemsBox{
    position: absolute;
    bottom: 20rpx;
    left: 30rpx;
    font-size: 20rpx;
    color: #ffffff;
}
