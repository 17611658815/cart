/* pages/feedback/feedback.wxss */

.minheight{
min-height: 100vh;
}
.areaBox {
    padding: 40rpx 0;
}

.picBox {
    width: 130rpx;
    height: 130rpx;
    display: inline-block;
    position: relative;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}

.delicon {
    width: 40rpx;
    height: 40rpx;
    text-align: center;
    line-height: 32rpx;
    font-size: 60rpx;
    border-radius: 50%;
    background: #f44040;
    color: #fff;
    position: absolute;
    right: 0rpx;
    top: 0rpx;
}

.inputBox {
    width: 690rpx;
    height: 325rpx;
    background: #f5f7f8;
    border-radius: 8rpx;
    margin: 0 auto 40rpx;
}

.input {
    width: 85%;
    padding: 40rpx 40rpx 40rpx 40rpx;
    height: 245rpx;
    margin: auto;
    font-size: 28rpx;
    color: #333;
    text-align: justify;
}

.phcolor {
    font-size: 28rpx;
    line-height: 100rpx;
    color: #999;
}

.btnBox {
    width: 690rpx;
    height: auto;
    margin: 0 auto;
}

.imgBox {
    width: 100%;
    height: auto;
    display: flex;
    flex-wrap: wrap;
}

.feedPic {
    width: 130rpx;
    height: 130rpx;
    border: 1rpx solid #cecece;
    margin-bottom: 30rpx;
}

.chooseImg {
    display: flex;
    width: 130rpx;
    height: 130rpx;
    color: #cecece;
    font-size: 80rpx;
    line-height: 130rpx;
    text-align: center;
    border-radius: 8rpx;
    font-weight: 100;
    border: 1rpx dashed #d1d1d1;
}

.chooseImg .add {
    width: 50px;
    height: 50px;
    margin: auto;
}

/* 提交 */
.submit {
    width: 90%;
    height: 100rpx;
    font-size: 34rpx;
    line-height: 100rpx;
    border-radius: 44px 44px 44px 44px;
    text-align: center;
    background: linear-gradient(#6eaff7, #6e9ff7);
    color: #fff;
}

button::after {
    border: none;
}
