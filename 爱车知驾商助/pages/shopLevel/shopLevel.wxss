/* pages/shopLevel/shopLevel.wxss */
.headerImg{
    width: 100%;
    height: 150rpx;
}
.listBox_item{
    width: 94%;
    padding: 20rpx 3%;
    display: flex;
    align-items: center;
    background: #ffffff;
    justify-content: space-between;
    margin-bottom: 20rpx;
}
.listBox_item_left image{
    width: 80rpx;
    height: 100rpx;
}
.listBox_item_center{
    flex: 1;
    margin-left: 20rpx;
}
.listBox_item_right{
    width: 120rpx;
    height: 50rpx;
    border-radius: 90rpx;
    background-image: linear-gradient(#F9903F, #F35235);
    font-size: 22rpx;
    color: #ffffff;
    line-height: 50rpx;
    text-align: center;
}
.listBox_item_right_2{
    width: 120rpx;
    height: 50rpx;
    border-radius: 90rpx;
    /* background-image: linear-gradient(#F9903F, #F35235); */
    background: #999999;
    font-size: 22rpx;
    color: #ffffff;
    line-height: 50rpx;
    text-align: center;
}