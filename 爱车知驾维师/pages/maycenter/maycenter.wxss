.container{background: #ffffff;}
.headerBox{
    width: 94%;
    padding: 30rpx 3%;
    display: flex;
}
.user_avatar{
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    border: 3rpx solid #DADCE6;
}
.user_avatar image{
    width: 60rpx;
    height: 60rpx;
    
}
.user_name{
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 10rpx;
    font-size: 28rpx;
    color: #DADCE6
}
.tab_box{
    width: 100%;
    display: flex;
    flex-wrap: wrap;
}
.tab_box_item image{
    width: 50rpx;
    height: 50rpx;
}
.tab_box_item{
    width: 25%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    font-size: 28rpx;
}