/* 卡卷样式 */

.bound {
  width: 250rpx;
  height: 40rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
  border: 2rpx solid #FD3F3F;
  text-align: center;
  line-height: 40rpx;
  color: #FD3F3F;
  font-weight: 500;
  font-size: 26rpx;
}

.bound::before {
  content: "";
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  display: block;
  background: #fff;
  position: absolute;
  top: 50%;
  left: -20rpx;
  transform: translateY(-50%);
  border-right: 2rpx solid #FD3F3F;
}

.bound::after {
  content: "";
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  display: block;
  background: #fff;
  position: absolute;
  top: 50%;
  right: -20rpx;
  transform: translateY(-50%);
  border-left: 2rpx solid #FD3F3F;
}

