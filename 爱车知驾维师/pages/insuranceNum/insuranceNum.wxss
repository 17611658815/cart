/* pages/insuranceNum/insuranceNum.wxss */
.headerImg{
    width: 100%;
    height: 650rpx;
}
.bottom_btn{
    width: 100%;
    height: 120rpx;
    background: yellow;
    position: fixed;
    bottom: 0;
    left: 0;
    padding-bottom: env(safe-area-inset-bottom); 
    display: flex;
    align-items: center;
}
.bottom_btn>view:nth-child(1){
    width: 60%;
    height: 100%;
    background: #ffffff;
    text-align: center;
    line-height: 120rpx;
    color: #F9903F;
}
.bottom_btn>view:nth-child(2){
    width: 40%;
    height: 100%;
    background:#F9903F;
    text-align: center;
    line-height: 120rpx;
    color: #ffffff;
}
.card_info{
    width: 85%;
    padding: 30rpx;
}
.card_info_msg{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.card_info_msg_left{
    width: 150rpx;
    height: 150rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.card_info_msg_left image{
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}
.card_info_msg_center{
    flex: 1;
    height: 130rpx;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.card_info_msg_center image{
    width: 45rpx;
    height: 45rpx;
}
.tagStyle{
    padding: 10rpx 10rpx;
    background: #333333;
    color: #ffffff;
    font-size: 20rpx;
    margin-left: 20rpx;
}
.card_info_msg_right{
    width: 50rpx;
    height: 140rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    text-align: center;
    border: 1rpx solid #999999;
    color: #999999;
    font-size: 28rpx;
}
.card_info_text{
    margin-top: 30rpx;
    color: #999999;
    font-size: 28rpx;
    line-height: 40rpx;
}