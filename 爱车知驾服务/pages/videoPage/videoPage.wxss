.navList{
    padding: 0 3%;
    background: #ffffff;
    border-bottom: 1rpx solid #e2e2e2;
    font-size: 28rpx;
    display: flex;
    justify-content: space-around;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 99;
}
.navList>view{
    padding-bottom: 15rpx;
}
.navActive{
    color: #F9903F;
    border-bottom: 5rpx solid #F9903F;
}