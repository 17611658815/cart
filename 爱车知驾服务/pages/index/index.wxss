/**index.wxss**/

.headerBox {
    position: relative;
    background: #fff;
    z-index: 10;
}

.user_nav image {
    width: 30rpx;
    height: 35rpx;
}

.mssage_icon_box {
    margin-right: 30rpx;
}

.mssage_icon_box image {
    width: 35rpx;
    height: 35rpx;
}
.type1{
    position: relative;
}
.delBox{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: -7rpx;
    right: 20rpx;
    z-index: 9999;
}
.delBox2{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 10rpx;
