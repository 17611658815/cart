}

.addres {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}

/* 滑动导航 */

.scroll-x-box {
    width: 85%;
    height: 80rpx;
    display: flex;
}

.scroll-x-content {
    width: 100%;
    background: #fff;
    display: flex;
    align-items: center;
    justify-content: center;
    white-space: nowrap;
    left: 0;
    z-index: 9;
    position: relative;
    
}

.scroll-x-content-item {
    display: inline-block;
    padding: 20rpx 0;
    margin-right: 50rpx;
    font-size: 30rpx;
    color: #2d2d2d;
    position: relative;
}

.active0 {
    font-size: 30rpx;
    color: #57be80;
    font-weight: 500;
}
.active1 {
    font-size: 30rpx;
    color: #250F98;
    font-weight: 500;
}
.active2 {
    font-size: 30rpx;
    color: #8D0387;
    font-weight: 500;
}
.active3 {
    font-size: 30rpx;
    color: #5C3E42;
    font-weight: 500
}

.defaultColor {
    font-size: 30rpx;
    color: #999;
}

.soloder {
    width: 40rpx;
    height: 6rpx;
    background: #000;
    margin-top: 10rpx;
    border-radius: 22rpx 22rpx 0 0;
}

.icon_down {
    width: 40rpx;
    height: 40rpx;
    margin-left: 20rpx;
}

/* 地图样式 */

.mapBox {
    width: 100%;
    height: 1000rpx;
    position: relative;
}

.hint_msg {
    width: 90%;
    background: #fff;
    position: absolute;
    bottom: -15%;
    left: 50%;
    transform: translate(-50%, -50%);
    border-radius: 6rpx;
    padding: 20rpx 20rpx;
}

.hint_icon {
    width: 60rpx;
    height: 60rpx;
    background: #f5f6f8;
    border-radius: 50%;
    justify-content: center;
}

.hint_text {
    padding: 0 20rpx;
}

.hint_text {
    flex: 1;
}

.hint_icon image {
    width: 35rpx;
    height: 35rpx;
}

.hint_btn {
    width: 150rpx;
    height: 60rpx;
    font-size: 26rpx;
    text-align: center;
    line-height: 60rpx;
    color: #999;
    border: 3rpx solid #c8c8c8;
}

/* 急叫中心 */

.call_Box {
    width: 230rpx;
    padding-right: 30rpx;
    height: 80rpx;
    position: absolute;
    background: #fff;
    bottom: 200rpx;
    left: 30rpx;
    text-align: right;
    font-size: 26rpx;
    color: #333;
    line-height: 80rpx;
    display: flex;
    justify-content: flex-end;
    border-radius: 44rpx;
}

.left_icon {
    width: 100rpx;
    height: 100rpx;
    background: #03a1ea;
    border-radius: 50%;
    position: absolute;
    left: 15%;
    top: 50%;
    transform: translate(-50%, -50%);
}

.right_icon {
    width: 15rpx;
    height: 15rpx;
}

/* 预约 */

.tabNav {
    width: 90%;
    display: flex;
    justify-content: space-evenly;
    font-size: 28rpx;
}

.tabNav view {
    width: 100rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    border-radius: 44rpx;
    color: #999;
    font-size: 28rpx;
}
.tabNav picker {
    width: 100rpx;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    border-radius: 44rpx;
    color: #999;
    font-size: 28rpx;
}

.border {
    border: 2rpx solid #c8c8c8;
    color: #000 !important;
}
.borderRed {
    border: 1rpx solid red;
    color: red !important;
}

.left_Box {
    width: 50rpx;
    height: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.right_text {
    flex: 1;
    height: 90rpx;
    line-height: 90rpx;
    border-bottom: 1rpx solid #e2e2e2;
}

/* 遮罩层 */

.shade_1 {
    width: 100%;
    min-height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    z-index: 10;
}

.shade_2 {
    width: 100%;
    min-height: 100vh;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    /* z-index: 9 */
}

/* 全部服务 */

.list_container {
    width: 100%;
    background: #fff;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 9999;
}

.icon_colse {
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top: 50%;
    left: 94%;
    transform: translate(-50%, -50%);
}

/* 服务列表 */

.server_item {
    width: 94%;
    border: 1rpx solid #e2e2e2;
    display: flex;
    flex-wrap: wrap;
}

.server_item_item {
    width: 33%;
    height: 200rpx;
    border-bottom: 1rpx solid #e2e2e2;
    border-right: 1rpx solid #e2e2e2;
}

.server_item_item .server_item_logo {
    width: 100rpx;
    height: 60rpx;
    background: papayawhip;
}

.server_item_item:nth-child(3n) {
    border-right: none !important;
}

.server_item_item:nth-child(n+13) {
    border-bottom: none !important;
}

