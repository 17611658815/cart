<view class="container">
    <view class="tabBox width100">
        <view style="margin-right:40rpx;" data-index='0' class="{{currentTab == 0 ? 'active':''}}" bindtap="switchTab">精品</view>
        <view data-index='1' class="{{currentTab == 1 ? 'active':''}}" bindtap="switchTab">最新</view>
    </view>
    <view class="width90 dpfC" style="background:#ffffff">
        <view class="width90 dpfb padding20">  
            <view class="dpf">
                <image class="city_icon" src="../../images/city_icon.png"></image>
                <view class="fontSize-36 fontWeight5 marginLeft">北京市</view>
                <view class="fontSize-26 color3 marginLeft">结果</view>
            </view>
            <view class="fontSize-26 color3">
                更多
            </view>
        </view>
        <view class="width100 dpfC">
            <view class="listBox width100 padding20 boxShadow" wx:for='{{searchData}}' bindtap="goShopDetaile">
                <view class="marginLeft10">
                     <image class="GPS_icon" src="../../images/GPS_icon.png"></image>
                </view>
                <view class="listBox_text white_noWrap marginLeft">
                    <view class="fontSize-28">
                      <searchHighlightTextView class="result-item" datas='{{searchData[index]}}' bindtap="chooseSearchResultAction" data-id="{{item.id}}" data-val='{{item.title}}' />
                    </view>
                    <view class="fontSize-26 color3 marginTop10">汽车服务店（华创生活广场店）汽车服务店（华创生活广场店）</view>
                    <view class="fontSize-26 color3 marginTop10">86人访问过</view>
                </view>
                <view class="dpfC marginRight10">
                    <view class="kmLogo"></view>
                    <view class="fontSize-22 color3 marginTop10">10.1km</view>
                </view>
            </view>
        </view>
    </view>
</view>

<view class="addBox">
    <image src="../../images/add_icon.png"> </image>
</view>