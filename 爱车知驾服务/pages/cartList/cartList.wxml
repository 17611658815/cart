<view class='container'>
    <view class="header">

    </view>
    <view class='goods_content width100 padding30 bgFFFF'>
        <view class="width90 dpfb">
            <view class="marginTop dpf">
                <image style="width:40rpx;height:40rpx;" src='../../images/checked.png'></image> 小保养服务</view>
            <view class="marginTop">
                优惠卷
            </view>
        </view>
        <view class="width85">1，更换润滑油规格，5w-30sn（4L）</view>
        <scroll-view scroll-x="true" class='scroll-x-box'>
            <view class='goods_content_item' wx:for='{{CartData1}}' wx:key='key'>
                <view class='dpf'>
                    <view class='dpf'>
                        <image style='width:24rpx;height:24rpx' src='../../../../images/icon/icon_common2.png'></image>
                    </view>
                </view>
                <view class='itemright'>
                    <view class='radioBox' data-index='{{index}}' bindtap='firstChecked'>
                        <image wx:if='{{item.checked}}' src='../../images/inon_checked.png'></image>
                    </view>
                    <view class='dpfC'>
                        <view class='leftImg'>
                            <image src='{{item.project_thumb}}'></image>
                        </view>
                        <view class='goodsContent'>
                            <view>
                                <view class='fontSize-32 color1 fontWeight5'>{{item.project_name}}</view>
                                <view class='fontSize-32 color1'>
                                    <text class='fontWeight5 color7'>{{item.price}}</text>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </scroll-view>
        <view class="width85">2，更换机油卢新规格：CX02-03</view>
        <scroll-view scroll-x="true" class='scroll-x-box'>
            <view class='goods_content_item' wx:for='{{CartData2}}' wx:key='key'>
                <view class='dpf'>
                    <view class='dpf'>
                        <image style='width:24rpx;height:24rpx' src='../../../../images/icon/icon_common2.png'></image>
                    </view>
                </view>
                <view class='itemright'>
                    <view class='radioBox' data-index='{{index}}' bindtap='secondChecked'>
                        <image wx:if='{{item.checked}}' src='../../images/inon_checked.png'></image>
                    </view>
                    <view class='dpfC'>
                        <view class='leftImg'>
                            <image src='{{item.project_thumb}}'></image>
                        </view>
                        <view class='goodsContent'>
                            <view>
                                <view class='fontSize-32 color1 fontWeight5'>{{item.project_name}}</view>
                                <view class='fontSize-32 color1'>
                                    <text class='fontWeight5 color7'>{{item.price}}</text>
                                </view>
                            </view>
                        </view>
                    </view>
                </view>
            </view>
        </scroll-view>
        <view class="width85">3，更换时间服务费</view>
        <view style="width:70%" class="dpfb marginTop">
            <view>上门:
                <text class="color7">￥150</text> </view>
            <view>来店:
                <text class="color7">￥150</text> </view>
        </view>
    </view>
    <view class="partsNum width90 marginTop dpfC bgFFFF" wx:for='{{3}}'>
        <view class="partsNum_item padding30">
            <view class="partsNum_item_left">
                <image style="width:40rpx;height:40rpx;" src='../../images/no_checked.png'></image>
            </view>
            <view class="partsNum_item_right flex_1">
                <view class="marginLeft10 dpfb">
                    <text>发动机清洗</text> 
                    <text>￥68</text> 
                </view>
                <view class="marginLeft10">50000km或6个月/次</view>
            </view>
        </view>
    </view>
</view>

