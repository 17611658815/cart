<view class='purchase dpfC'>
    <view style='width:95%;' class='dpf'>
        <view class='purchase_price dpf'>
            <view class="dpfC service">
                <image style="width:45rpx;height:45rpx" src="../../images/kefu_icon.png"></image>
                <view class='fontSize-22 color2'>在线客服</view>
            </view>
            <view class="flex_1 dpfC">
                <view class="purchase_text">本次保养最多仅需：
                    <text class="color7 fontWeight6 fontSize-45">￥6666</text> </view>
                <view>节省：633元</view>
            </view>
            <!-- <view class='fontSize-30 color1'>合计：</view>
            <view class='fontSize-30 color4 fontWeight5'>{{total}}元</view> -->
        </view>
        <view class='purchase_btn' catchtap='placeOther'>去结算</view>
    </view>
    <view class='isIphoneXBox' wx:if='{{isIphoneX}}'></view>
</view>